{
  "name": "aptis-backend",
  "version": "1.0.0",
  "description": "APTIS Exam System Backend API",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "seed": "node src/seeds/index.js",
    "seed:init": "node src/seeds/01-init-database.js",
    "seed:types": "node src/seeds/02-seed-types.js",
    "seed:users": "node src/seeds/03-seed-users.js",
    "seed:ai": "node src/seeds/04-seed-ai-criteria.js",
    "seed:questions": "node src/seeds/05-seed-questions.js",
    "seed:exams": "node src/seeds/06-seed-exams.js",
    "seed:complete": "npm run seed:init && npm run seed:types && npm run seed:users && npm run seed:ai && npm run seed:questions && npm run seed:exams",
    "seed:reset": "npm run seed:complete",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "aptis",
    "exam",
    "api",
    "express"
  ],
  "author": "",
  "license": "ISC",
